{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true
  },
  "exclude": [
      "node_modules"
  ],
  "include": [
    "src/**/*"
  ]
}
